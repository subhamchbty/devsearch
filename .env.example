# Environment variables for DevSearch local development
# Copy this file to .env and fill in the values

# Database
DATABASE_URL=postgresql://devsearch:devsearch@db:5432/devsearch

# Redis
REDIS_URL=redis://redis:6379

# Node
NODE_ENV=development

# Service Ports
CLIENT_PORT=3000
CRAWLER_PORT=3001
EMBEDDER_PORT=3002
ENGINE_PORT=3003
INDEXER_PORT=3004

# Embedding Provider (Phase 1)
# OPENAI_API_KEY=your-key-here
# EMBEDDING_MODEL=text-embedding-3-small
